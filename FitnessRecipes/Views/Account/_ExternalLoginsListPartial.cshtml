@using (Html.BeginForm("ExternalLogin", "Account", new { ReturnUrl = ViewBag.ReturnUrl }))
{
    @Html.AntiForgeryToken()
    <h4 class="info_green">Logg inn med en annen tjeneste</h4>
    <br />
    <br />
    <a href="/Account/RedirectToAuthenticate?providerKey=Twitter">
        <img src="/Content/themes/base/images/1358652820_twitter.png" /></a>
    <a href="/Account/RedirectToAuthenticate?providerKey=Facebook">
        <img src="/Content/themes/base/images/1358652758_facebook.png" /></a>
    <a href="/Account/RedirectToAuthenticate?providerKey=Google">
        <img src="/Content/themes/base/images/1358652735_google.png" /></a>
}
